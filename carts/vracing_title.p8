local vrtext=""
local vrbytes={}
for i=1,#vrtext,8 do
	add(vrbytes,tonum("0x"..sub(vrtext,i,i+3).."."..sub(vrtext,i+4,i+7)))
end
function draw_slide(t)
	t=smoothstep(t)
	local rows=flr(64*t)
	local skip_rows=63-rows
	local mem=0x6000
	for i=0,rows-1 do
		for k=0,15 do
			poke4(mem,vrbytes[(skip_rows+i)*16+k+1])
			mem+=4
		end
	end
	local mem=0x6000+(128-rows)*64
	for i=0,rows-1 do
		for k=0,15 do
			poke4(mem,vrbytes[(64+i)*16+k+1])
			mem+=4
		end
	end
end

function slidein_state(after_state,params)	
	local ttl=20
	return {
		-- draw
		draw=function()
			draw_slide(1-ttl/20)
		end,
		update=function(self)
			ttl-=1
			if ttl<0 then
				-- current background
				pop_state()
				-- self
				pop_state()
				-- next active
				push_state(after_state,params)
				-- transition
				push_state(slideout_state)
			end
		end
	}
end

function slideout_state()
	local ttl=20
	return {
		-- draw
		draw=function()
			draw_slide(ttl/20)
		end,
		update=function(self)
			ttl-=1
			if ttl<0 then
				pop_state()
			end
		end
	}
end

